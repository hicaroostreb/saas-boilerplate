{
  "name": "@workspace/requirements-check",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Enterprise development requirements validator for monorepo",
  "bin": {
    "check-requirements": "./bin/check.js"
  },
  "scripts": {
    "check": "node ./bin/check.js",
    "check:verbose": "node ./bin/check.js --verbose",
    "check:strict": "node ./bin/check.js --strict",
    "format": "prettier --write ."
  },
  "dependencies": {
    "semver": "^7.5.0",
    "chalk": "^5.3.0",
    "which": "^4.0.0"
  },
  "devDependencies": {
    "prettier": "^3.6.2"
  },
  "keywords": [
    "requirements",
    "validation",
    "development",
    "enterprise",
    "monorepo"
  ],
  "author": "SaaS Team",
  "license": "MIT"
}
